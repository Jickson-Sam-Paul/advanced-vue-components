{
  "name": "advanced-components",
  "version": "1.0.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/advanced-components.cjs.js",
  "module": "./dist/advanced-components.es.js",
  "types": "./dist/lib.d.ts",
  "exports": {
    ".": {
      "types": "./dist/lib.d.ts",
      "import": "./dist/advanced-components.es.js",
      "require": "./dist/advanced-components.cjs.js"
    },
    "./style.css": "./dist/advanced-components.css",
    "./dist/advanced-components.css": "./dist/advanced-components.css"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build --config vite.lib.config.ts",
    "build:app": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "pack:local": "npm run build && npm pack"
  },
  "dependencies": {
    "dompurify": "^3.2.6",
    "markdown-it": "^14.1.0",
    "plotly.js-dist-min": "^3.1.0",
    "prismjs": "^1.30.0"
  },
  "peerDependencies": {
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^8.6.12",
    "@storybook/addon-docs": "^8.6.12",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/vue3-vite": "^8.6.12",
    "@types/node": "^22.13.10",
    "@vitejs/plugin-vue": "^5.2.1",
    "storybook": "^8.6.12",
    "typescript": "^5.8.2",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.4",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.8"
  }
}
